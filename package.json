{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.10.0",
        "@emotion/styled": "^11.10.0",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.9.3",
        "@mui/styled-engine-sc": "^5.9.3",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "axios": "^0.27.2",
        "chokidar": "^3.5.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "styled-components": "^5.3.5",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "WATCHPACK_POLLING=true=true react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --passWithNoTests",
        "eject": "react-scripts eject",
        "compose:up": "docker-compose up",
        "compose:down": "cd ./docker && docker-compose down --rmi all -v",
        "compose:up:dev": "docker-compose -f ./docker/docker-compose.dev.yaml up --build",
        "compose:up:prod": "GITHUB_SHA=$(git rev-parse --short HEAD) docker-compose -f ./docker/docker-compose.prod.yaml up --build -d --force-recreate --remove-orphans"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "dotenv": "^16.0.1"
    }
}
